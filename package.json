{
  "name": "vue-my-photos",
  "version": "3.0.1",
  "description": "Simple lightbox component for Vue 3 applications",
  "author": "Andrew McGrath",
  "scripts": {
    "serve-demo": "vue-cli-service serve demo/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/lightbox.ssr.js",
  "module": "dist/lightbox.esm.js",
  "browser": "dist/lightbox.esm.js",
  "unpkg": "dist/lightbox.min.js",
  "types": "lightbox.d.ts",
  "files": [
    "dist/",
    "lightbox.d.ts",
    "src/**/*.vue"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "typescript": "^3.8.3",
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/am283721/vue-my-photos",
  "keywords": [
    "vue",
    "vue.js",
    "vue3",
    "lightbox",
    "image",
    "photo",
    "gallery"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/am283721/vue-my-photos.git"
  }
}